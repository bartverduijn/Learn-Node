{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
  },

  "rules": {
    "no-tabs": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "import/newline-after-import": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    },

  "extends": "airbnb-base",
  "plugins": [
    "import"
  ]
}
